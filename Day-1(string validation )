#include <stdio.h>
#include <string.h>
int main() 
{
	printf("192372128 / Revanth .\n");
    char inputString[50];
    printf("Enter a string: ");
    scanf("%s", inputString);
    
    if (strlen(inputString) > 0)
	{
        printf("Valid string entered.\n");
    } else {
        printf("Invalid string entered.\n");
    }
    return 0;
}
