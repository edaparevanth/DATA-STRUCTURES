#include <stdio.h>
int main() 
{
	printf("Revanth / 192372128 .\n");
    char arr[5] = {'d', 'a', 'c', 'b', 'e'};
    int i, j;
    char temp;

    for (i = 0; i < 5; i++) 
	{
        for (j = i + 1; j < 5; j++) 
		{
            if (arr[i] > arr[j]) 
			{
                temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }

    printf("Ascending Order: ");
    for (i = 0; i < 5; i++) 
	{
        printf("%c ", arr[i]);
    }

    for (i = 0; i < 5; i++) {
        for (j = i + 1; j < 5; j++) 
		{
            if (arr[i] < arr[j]) 
			{
                temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
            }
        }
    }

    printf("\nDescending Order: ");
    for (i = 0; i < 5; i++) 
	{
        printf("%c ", arr[i]);
    }
    return 0;
}
