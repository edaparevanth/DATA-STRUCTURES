#include "stdio.h"
int main()
{
	printf("Revanth / 12372128.\n");
	int m,n;
	printf("Enter the number of rows : ");
	scanf("%d",&m);
	printf("Enter the number if columns : ");
	scanf("%d",&n);
	printf("Enter the elements for matrix  :\n");
    int	mat1[m][n];
	for(int i=0;i<m;i++)
	{
		for(int j=0;j<n;j++)
		{
			scanf("%d",&mat1[i][j]);
		}
	}
printf("Transpose of matrix  :\n");
for(int i=0;i<m;i++)
{
	for(int j=0;j<n;j++)
	{
		printf("%d ",mat1[j][i]);
	}
	printf("\n");
}
return 0;
}
