#include<stdio.h>
int main() 
{
    printf("Revanth / 19272128 .\n);
    int n;
    printf("Enter the size of array: ");
    scanf("%d",&n);
    int arr[n];
    printf("\nEnter the array elements: ");
    for(int i=0;i<n;i++) 
    {
        scanf("%d",&arr[i]);
    }

    printf("\nEven numbers in the array: ");
    for(int i=0;i<n;i++) 
    {
        if(arr[i] % 2 == 0) 
        {
            printf("%d ", arr[i]);
        }
    }

    printf("\nOdd numbers in the array: ");
    for(int i=0;i<n;i++) 
    {
        if(arr[i] % 2 != 0) 
        {
            printf("%d ", arr[i]);
        }
    }

    return 0;
}
