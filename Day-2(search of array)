#include <stdio.h>
int main() 
{
	printf("Revanth / 192372128 .\n");
    int array[] = {2, 4, 0, 1, 5, 9, 8, 7, 3, 6};
    int target = 5;
    int n = sizeof(array) / sizeof(array[0]);
    int i, found = 0;

    for (i = 0; i < n; i++) 
	{
        if (array[i] == target) 
		{
            found = 1;
            break;
        }
    }

    if (found)
        printf("Element found at index: %d", i);
    else
        printf("Element not found");

    return 0;
}
